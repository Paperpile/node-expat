  "keywords": [
    "xml",
    "sax",
    "expat",
    "libexpat",
    "parse",
    "parsing"
  ],
  "scripts": {
    "preversion": "npm test",
    "lint": "standard",
    "unit": "vows --spec ./test/**/*.js",
    "test": "npm run unit && npm run lint",
